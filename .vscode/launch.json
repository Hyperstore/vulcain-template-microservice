{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "node",
			"request": "launch",
			"smartStep": true,
			"program": "${workspaceRoot}/src/index.ts",
			"stopOnEntry": false,
			"preLaunchTask": "compile-ts",
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
			],
			"env": {
				"VULCAIN_ENV": "<%=env%>",
				"VULCAIN_SERVICE_NAME": "<%= project.fullName %>",
				"VULCAIN_SERVICE_VERSION": "1.0",
				"VULCAIN_TEST": "true",
				"NODE_ENV": "development"
			},
            "console": "integratedTerminal",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/dist"
		},
        {
            "name": "Test",
            "type": "node",
			"request": "launch",
            "smartStep": true,
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "preLaunchTask": "compile-test",
            "args": ["${workspaceRoot}/dist-test/**/*.js"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
            ],
            "env": {
				"VULCAIN_ENV": "<%=env%>",
				"VULCAIN_SERVICE_NAME": "<%= project.fullName %>",
				"VULCAIN_SERVICE_VERSION": "1.0",
				"VULCAIN_TEST": "True",
                "VULCAIN_TEST": "true"
            },
            "console": "integratedTerminal",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/dist-test"
        },
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}
